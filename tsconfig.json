{
    "compilerOptions": {
      "target": "es6",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "esModuleInterop": true,      
      "strict": false,      
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": false,      
      "jsx": "react-jsx",
      "baseUrl": "./"
    },
    "include": [
      "src"
    ],
    "exclude": []
  }
  